{"ast":null,"code":"import { useEffect } from \"react\";\nexport const useOnClickOutside = (ref, handler) => {\n  useEffect(() => {\n    const listener = e => {\n      if (!ref.current || ref.current.contains(e.target)) {\n        return;\n      }\n\n      handler(e);\n    };\n\n    document.addEventListener(\"mousedown\", listener);\n    return () => {\n      document.removeEventListener(\"mousedown\", listener);\n    };\n  }, [ref, handler]);\n};","map":{"version":3,"sources":["/Users/tangled/Programming/PROJECTS/edible-wild-world-next-prismic/edible-wild-world-next/components/hooks.js"],"names":["useEffect","useOnClickOutside","ref","handler","listener","e","current","contains","target","document","addEventListener","removeEventListener"],"mappings":"AAAA,SAASA,SAAT,QAA0B,OAA1B;AAEA,OAAO,MAAMC,iBAAiB,GAAG,CAACC,GAAD,EAAMC,OAAN,KAAkB;AACjDH,EAAAA,SAAS,CAAC,MAAM;AACd,UAAMI,QAAQ,GAAIC,CAAD,IAAO;AACtB,UAAI,CAACH,GAAG,CAACI,OAAL,IAAgBJ,GAAG,CAACI,OAAJ,CAAYC,QAAZ,CAAqBF,CAAC,CAACG,MAAvB,CAApB,EAAoD;AAClD;AACD;;AACDL,MAAAA,OAAO,CAACE,CAAD,CAAP;AACD,KALD;;AAMAI,IAAAA,QAAQ,CAACC,gBAAT,CAA0B,WAA1B,EAAuCN,QAAvC;AACA,WAAO,MAAM;AACXK,MAAAA,QAAQ,CAACE,mBAAT,CAA6B,WAA7B,EAA0CP,QAA1C;AACD,KAFD;AAGD,GAXQ,EAWN,CAACF,GAAD,EAAMC,OAAN,CAXM,CAAT;AAYD,CAbM","sourcesContent":["import { useEffect } from \"react\";\n\nexport const useOnClickOutside = (ref, handler) => {\n  useEffect(() => {\n    const listener = (e) => {\n      if (!ref.current || ref.current.contains(e.target)) {\n        return;\n      }\n      handler(e);\n    };\n    document.addEventListener(\"mousedown\", listener);\n    return () => {\n      document.removeEventListener(\"mousedown\", listener);\n    };\n  }, [ref, handler]);\n};\n"]},"metadata":{},"sourceType":"module"}